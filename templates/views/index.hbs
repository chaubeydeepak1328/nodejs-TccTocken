  }

  body {
    background-color: black;
  }

  .card {
    height: 100%;
    padding: 20px;
    border-radius: 10px;
    background:
      linear-gradient(90deg, #1b121d 0, #120f2f 50%, #1b121d 100%);
    ;
    border: 1px solid rgba(255, 255, 255, .15);
  }

  footer {
    background: linear-gradient(90deg,
        #1b121d 0, #120f2f 50%, #1b121d 100%);
  }

  .container {
    display: flex;
    flex-direction: row;
    width: 100%;
    align-items: center;
    justify-content:
      center;
    max-width: 960px;
    margin: auto;

    #inviteCode.invite-page {
      box-sizing: border-box;
      display: flex;
      flex-direction: row;
      background-color: transparent;
      border: none;
      padding: 10px;
      margin-bottom:
        10px;
      border-radius: 5px;
      justify-content: space-between;
      width: 100%;
      box-shadow: 0px 1px 2px rgba(0, 0, 0, .07);

      #link {
        align-self: center;
        font-size: 15px;
        color: #9097a7;
        font-weight: 600;
        flex-grow: 2;
        background-color: transparent;
        border: none;
      }

      #copy {
        width: 30px;
        height:
          30px;
        margin-left: 20px;
        border: 1px solid #9097a7;
        border-radius: 5px;
        background-color: transparent;

        i {
          display: block;
          line-height: 30px;
          position: relative;

          &::before {
            display: block;
            width: 15px;
            margin: 0 auto;
          }

          &.copied::after {
            position: absolute;
            top: 0px;
            right: 35px;
            height: 30px;
            line-height: 25px;
            display: block;
            content: "copied";
            font-size: 1.5em;
            padding: 2px 10px;
            color: #9097a7;
            background-color: #4099FF;
            border-radius:
              3px;
            opacity: 1;
            will-change: opacity, transform;
            animation: showcopied 1.5s ease;
          }
        }

        &:hover {
          cursor: pointer;
          background-color: darken(#f8f8f8, 10%);
          transition: background-color .3s ease-in;
        }
      }
    }
  }

  @keyframes showcopied {
    0% {
      opacity: 0;
      transform: translateX(100%);
    }

    70% {
      opacity: 1;
      transform:
        translateX(0);
    }

    100% {
      opacity: 0;
    }
  }
</style>

