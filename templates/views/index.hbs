      #copy {
        width: 30px;
        height:
          30px;
        margin-left: 20px;
        border: 1px solid #9097a7;
        border-radius: 5px;
        background-color: transparent;

        i {
          display: block;
          line-height: 30px;
          position: relative;

          &::before {
            display: block;
            width: 15px;
            margin: 0 auto;
          }

          &.copied::after {
            position: absolute;
            top: 0px;
            right: 35px;
            height: 30px;
            line-height: 25px;
            display: block;
            content: "copied";
            font-size: 1.5em;
            padding: 2px 10px;
            color: #9097a7;
            background-color: #4099FF;
            border-radius:
              3px;
            opacity: 1;
            will-change: opacity, transform;
            animation: showcopied 1.5s ease;
          }
        }

        &:hover {
          cursor: pointer;
          background-color: darken(#f8f8f8, 10%);
          transition: background-color .3s ease-in;
        }
      }
    }
  }

  @keyframes showcopied {
    0% {
      opacity: 0;
      transform: translateX(100%);
    }

    70% {
      opacity: 1;
      transform:
        translateX(0);
    }

    100% {
      opacity: 0;
    }
  }
