<script src="/newdist6/bundle.js"></script>
<script>
  var xValues = ["Reserved for exclusive listings", "Public Sell", "First-time exchange listing", "Gaming Ecosystem", "Airdrop Distribution", "Development Initiatives", "Market Exchange Management"];
  var yValues = [100, 25, 25, 2, 6, 9, 19];
  var barColors = [
    "#800080",
    "#e6e6fa",
    "#d8bfd8",
    "#dda0dd",
    "#b768a2",
    "#9400d3",
    "#9f00c5"
  ];

  new Chart("myChart", {
    type: "doughnut",
    data: {
      labels: xValues,
      datasets: [{
        backgroundColor: barColors,
        data: yValues
      }]
    },
    options: {
      plugins: {
        legend: {
          labels: {
            color: "white" // Set the legend text color to white
          }
        }
      },
      scales: {
        x: {
          ticks: {
            color: "white" // Set the x-axis label text color to white
          }
        },
        y: {
          ticks: {
            color: "white" // Set the y-axis label text color to white
          }
        }
      }
    }
  });
</script>



